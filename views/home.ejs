</head>

<body>
  <%- include('./partials/sideBar'); %>

  <div class="mainWrap col-md-6">
    <div>
      <h1 class="about-me">About Me</h1>
    </div>
    <div class="ps-wrap">
      <p class='psSum'>Professional Summary</p>
      <p class='psSum1'>
        <%= myPs[0].item %>
      </p>
    </div>
    <div class="ps-wrap">
      <p class='psSum'>Work Experience</p>
      <ul class="DetailsUL">
        <% for(var i = 1;i<myPs.length-1;i++){%>
        <li>
            <%= myPs[i].item %>
        </li>
        <%}%>
      </ul>
    </div>
      <div class="ps-wrap">
      <p class='psSum'>Personal Exploration</p>
      <ul class="DetailsUL">
        <%= myPs[myPs.length-1].item %>
      </ul>
    </div>

  </div>
</body>

